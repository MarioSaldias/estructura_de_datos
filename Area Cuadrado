// Area Cuadrado.cpp : Defines the entry point for the console application.

#include "stdafx.h"
#include<iostream>
#include"conio.h"
using namespace std;

void main()
{
	float lado,area;
	cout<<"Ingrese la longitud del lado del cuadrado: ";cin>>lado;
	area=lado*lado;
	cout<<"El area del cuadrado es: "<<area;
	getch();
}
